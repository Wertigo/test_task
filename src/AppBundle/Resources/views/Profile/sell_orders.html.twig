{% if orders is not empty %}
    <p>Sell orders</p>
    <table class="table table-bordered table-hover">
        <tr>
            <th>id</th>
            <th>Product</th>
            <th>Price</th>
        </tr>
        {% for order in orders %}
            <tr>
                <th>{{ order.id }}</th>
                <th>{{ order.product }}</th>
                <th>{{ order.price }}</th>
            </tr>
        {% endfor %}
    </table>
{% else %}
    <p>No sell orders</p>
{% endif %}